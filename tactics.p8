pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
select = {x = 0, y = 0}
chosen = {}
moving = false
gridsize = {x = 16, y = 16}
bg = {}
fg = {}

levels = {}
levels[1] = 0
levels[2] = 50

colors = {
  good = 11,
  evil = 14
}

sprites = {
  knight = {
    good = 1,
    evil = 17
  },
  dwarf = {
    good = 2,
    evil = 18
  },
  lancer = {
    good = 3,
    evil = 19
  },
  archer = {
    good = 4,
    evil = 20
  }
}

knight = {
  name = "knight",
  speed = 2,
  maxhp = 10,
  hp = 10,
  xp = 0,
  level = 1
}

dwarf = {
  name = "dwarf",
  speed = 2,
  maxhp = 15,
  hp = 15,
  xp = 0,
  level = 1
}

lancer = {
  name = "lancer",
  speed = 3,
  maxhp = 10,
  hp = 10,
  xp = 0,
  level = 1
}

archer = {
  name = "archer",
  speed = 4,
  maxhp = 5,
  hp = 5,
  xp = 0,
  level = 1
}

function _init()
  gridclear(bg)
  gridclear(fg)

  fg[0][0] = unit(knight, "good")
  fg[0][0].hp = 7

  fg[0][1] = unit(dwarf, "good")
  fg[0][1].hp = 2

  fg[1][4] = unit(dwarf, "evil")
  fg[1][4].hp = 7

  fg[0][3] = unit(archer, "good")
  fg[0][3].hp = 5
end

function _update()
  if btnp(0)
  and select.y > 0 then
    select.y -= 1
  end
  if btnp(1)
  and select.y < 15 then
    select.y += 1
  end
  if btnp(2)
  and select.x > 0 then
    select.x -= 1
  end
  if btnp(3)
  and select.x < 15 then
    select.x += 1
  end
  if btnp(4) then
    if moving == false
    and fg[select.x][select.y].sprite != 0 then
      chosen.x = select.x
      chosen.y = select.y
      valid = {}
      range(chosen.x, chosen.y, fg[chosen.x][chosen.y].speed)
      valid[chosen.x][chosen.y] = nil
      moving = true
    elseif moving == true
    and valid[select.x] != nil
    and valid[select.x][select.y] != nil then
      fg[select.x][select.y] = copy(fg[chosen.x][chosen.y])
      fg[chosen.x][chosen.y] = {sprite = 0}
      gridclear(bg)
      moving = false
    end  
  end 
end

function _draw()
  cls()
  griddraw(bg)
  griddraw(fg)
  selectdraw()
end

function gridclear(grid)
  for i=0, gridsize.x do
    grid[i] = {}
    for j=0, gridsize.y do
      grid[i][j] = {sprite = 0}
    end
  end
end

function griddraw(grid)
  for i=0, gridsize.x do
    for j=0, gridsize.y do
      pos = spritepos(grid[i][j].sprite)
      sspr(pos.x * 8, pos.y * 8, 8, 8, j * 8, i * 8)
    end
  end
end

function selectdraw()
  spr(255, select.y * 8, select.x * 8)
  if fg[select.x][select.y].sprite != 0 then
    showstats(select.x, select.y)
  end
end

function spritepos(s)
  sprite = {}
  sprite.x = s % 16
  sprite.y = flr(s / 16)
  return sprite
end

function showstats(x, y)
  unit = fg[x][y]
  print(unit.name, 100, 0, colors[unit.alignment])
  print("lvl. 1", 100, 8, colors[unit.alignment])
  health = unit.hp / unit.maxhp * 28
  rect(100, 16, 100 + health, 17, colors[unit.alignment])
  rect(100 + health, 16, 128, 17, 2)
end

function unit(base, alignment)
  new = copy(base)
  new.alignment = alignment
  new.sprite = sprites[base.name][alignment]
  return new
end

function copy(src)
  dest = {}
  dest.name = src.name
  dest.sprite = src.sprite
  dest.speed = src.speed
  dest.maxhp = src.maxhp
  dest.hp = src.hp
  dest.xp = src.xp
  dest.level = src.level
  dest.alignment = src.alignment
  return dest
end

function range(x, y, steps)
  if valid[x] == nil then
    valid[x] = {}
  end
 
  if valid[x][y] == nil
  or valid[x][y] < steps then
    valid[x][y] = steps
  else
    return
  end
  
  bg[x][y] = {sprite = 254}
  
  if y - 1 >= 0
  and steps > 0
  and fg[x][y - 1].sprite == 0 then
    range(x, y - 1, steps - 1)
  end

  if y + 1 < 16
  and steps > 0
  and fg[x][y + 1].sprite == 0 then
    range(x, y + 1, steps - 1)
  end

  if x - 1 >= 0
  and steps > 0
  and fg[x - 1][y].sprite == 0 then
    range(x - 1, y, steps - 1)
  end

  if x + 1 < 16
  and steps > 0
  and fg[x + 1][y].sprite == 0 then
    range(x + 1, y, steps - 1)
  end
end
